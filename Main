local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")

local PlaceId = game.PlaceId

local Games = {
    OperationOne = {
        Id = 72920620366355,
        GitHubLink = "https://raw.githubusercontent.com/0x3u/Loader/refs/heads/main/Scripts/OperationOne"
    },
    Flick = {
        Id = 136801880565837,
        GitHubLink = "https://raw.githubusercontent.com/0x3u/Loader/refs/heads/main/Scripts/Flick"
    }
}

local function LoadScript(url)
    local success, result = pcall(function()
        return loadstring(game:HttpGet(url))()
    end)

    if not success then
        warn("Failed to load script:", result)
    end
end

for name, data in pairs(Games) do
    if PlaceId == data.Id then
        LoadScript(data.GitHubLink)
        return
    end
end

warn("No script found for this PlaceId:", PlaceId)
