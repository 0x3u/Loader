local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")

local GameId = game.GameId

local OperationOne = {
    Id = 72920620366355,
    GitHubLink = "https://raw.githubusercontent.com/0x3u/Loader/refs/heads/main/Scripts/OperationOne"
}

local function LoadScript(url)
    local success, result = pcall(function()
        return loadstring(game:HttpGet(url))()
    end)

    if not success then
        warn("Failed to load script:", result)
    end
end

if GameId == OperationOne.Id then
    LoadScript(OperationOne.GitHubLink)
elseif GameId == OperationTwo.Id then
    LoadScript(OperationTwo.GitHubLink)
elseif GameId == OperationThree.Id then
    LoadScript(OperationThree.GitHubLink)
else
    warn("No script found for this Game ID:", GameId)
end
